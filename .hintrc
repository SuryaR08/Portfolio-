{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "definition-list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "cursor: -webkit-grab",
          "cursor: -webkit-grabbing"
        ]
      }
    ]
  }
}